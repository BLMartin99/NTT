proc split_list(list input_list) 
{
    int N = size(input_list);

    if (N % 2 != 0)
    {
        ("List size is not even");
        quit;
    }

   int i;
   list evens;
   list odds;

   for(i = 1; i < N; i = i + 1)
   {
        insert(evens, input_list[i]); 
        insert(odds,  input_list[i + 1]); 
   }

   list return_list;
   insert(return_list, evens);
   insert(return_list, odds);

   return(return_list);
}
